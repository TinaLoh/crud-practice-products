<%= link_to "Add New Product", new_product_path %>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Price</th>
      <th>Created At</th>
      <th>Updated At</th>

    </tr>
  </thead>

  <tbody>
    <% @products.each do |product| %>
    <tr>
      <td><%= link_to product.name, product_path(product) %></td>
      <td><%= product.description %></td>
      <td><%= product.price %></td>
      <td><%= product.created_at %></td>
      <td><%= product.updated_at %></td>

    </tr>
  <% end %>
  </tbody>
</table>
